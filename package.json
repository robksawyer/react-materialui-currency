{
  "name": "react-materialui-currency",
  "version": "1.0.1",
  "description": "Currency TextField for Material-UI",
  "main": "lib/CurrencyField/index.js",
  "scripts": {
    "build": "yarn run lint && yarn test && babel ./src --ignore *-test.js --out-dir ./lib",
    "prebuild": "rimraf lib",
    "test": "jest",
    "lint": "eslint --ext .jsx src && echo \"eslint: no lint errors\""
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helderfarias/react-materialui-currency.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "material design"
  ],
  "author": "Helder Farias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helderfarias/react-materialui-currency/issues"
  },
  "homepage": "https://github.com/helderfarias/react-materialui-currency#readme",
  "peerDependencies": {
    "material-ui": "^0.19.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-tap-event-plugin": "^1.0.0 || ^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-jsx-strip-ext": "^1.0.1",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "eslint": "^4.5.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.0.0",
    "jest-cli": "^20.0.4",
    "react-addons-test-utils": "^15.6.0",
    "rimraf": "^2.6.1",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "jest": "^20.0.4",
    "material-ui": "^0.19.0",
    "parse-float": "^0.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
